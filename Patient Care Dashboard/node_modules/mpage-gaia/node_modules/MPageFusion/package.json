  "scripts": {
    "automation-sandbox": "nodemon --watch automation --exec \"npm run build:test-helpers && node --inspect ./automation/dev/Sandbox.js\"",
    "build": "webpack --config ./build/webpack/config/webpack.mpage.config.js",
    "build:dev": "webpack --config ./build/webpack/config/webpack.mpage.config.js --env.TYPE=DEV",
    "build:dist": "webpack --config ./build/webpack/config/webpack.dist.config.js",
    "build:lib": "webpack --config ./build/webpack/config/webpack.lib.config.js",
    "build:test-helpers": "babel --presets babel-preset-mpage -d testing/ automation/ --ignore automation/__tests__,automation/dev",
    "deploy:site": "curl -X POST https://jenkins.cerner.com/discernabu/job/mpages-fusion-site/build --user $EC_USERNAME:$EC_PASSWORD",
    "dev": "node ./build/webpack/webpack-dev-server.js",
    "dev-build": "webpack --config ./build/webpack/config/webpack.mpage.config.js --watch --devtool source-map --env.TYPE=DEV",
    "docs": "jsdoc2md ./automation/**/*.js > ./docs/mpagefusion/testing/README.md",
    "gaia:server": "gaia server",
    "graybox-test": "jest --config ./automation/__tests__/jest.config.js --verbose --color",
    "graybox-test:watch": "jest --config ./automation/__tests__/jest.config.js --verbose --color --watch",
    "icons:update": "node ./build/icons/update.js",
    "lint": "npm run lint:source && npm run lint:styles",
    "lint:fix": "eslint src automation docs/demos --color --fix",
    "lint:source": "eslint src automation docs/demos --color --max-warnings 0",
    "lint:styles": "stylelint src/main/less/**/*.less",
    "prepublish": "in-publish && npm run build && npm run build:lib && npm run build:dist && npm run build:test-helpers || not-in-publish",
    "release:major": "fusion-make major --ec-profile npm --ec-profile pom",
    "release:minor": "fusion-make minor --ec-profile npm --ec-profile pom",
    "release:patch": "fusion-make patch --ec-profile npm --ec-profile pom",
    "test": "karma start --single-run --no-auto-watch ./build/karma/karma.config.js",
    "test:chrome": "karma start --single-run --no-auto-watch ./build/karma/karma.config.js --browsers Chrome",
    "test:ie": "karma start --single-run --no-auto-watch ./build/karma/karma.config.js --browsers IE",
    "test:noclose": "karma start ./build/karma/karma.config.nocoverage.js --no-single-run --browsers Chrome",
    "test:nocloseie": "karma start --no-single-run ./build/karma/karma.config.js --browsers IE"
  },
