        </arguments>
        <arguments name="VALUE1" dataType="CHARACTER" required="true" reconcileScript="N/A"/>
        <arguments name="OPT_VALUE2" dataType="CHARACTER" reconcileScript="N/A"/>
      </templateDefinitions>
