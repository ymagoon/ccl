  <ruleDefinition>
    <evokeSection>
      <evokes evokeEvent="//@expertTemplates/@events/@event.72">
        <logicExpression xsi:type="EM:Antecedent" id="E1" template="EKS_SYNCH_TRIGGER_E">
          <facts argument="TRIGGER">
            <value xsi:type="EM:ListValue">
              <values xsi:type="EM:ListValue">
                <values xsi:type="EM:CustomValue" hideDisplay="true">
                  <customValue xsi:type="EM:StringValue" value=" "/>
                </values>
                <values xsi:type="EM:StringValue" value="MCDS_ca_roc"/>
              </values>
            </value>
          </facts>
        </logicExpression>
      </evokes>
    </evokeSection>
    <logicSection>
      <logicExpression xsi:type="EM:InfixExpression">
        <leftExpression xsi:type="EM:Antecedent" id="L1" template="EKS_STOP_LOGIC_L">
          <facts argument="MODE">
            <value xsi:type="EM:StringValue" value="STOP"/>
          </facts>
        </leftExpression>
        <rightExpression xsi:type="EM:GroupExpression">
          <expression xsi:type="EM:InfixExpression" operator="OR">
            <leftExpression xsi:type="EM:GroupExpression">
              <expression xsi:type="EM:Antecedent" id="L2" template="EKS_AGE_EVAL_L">
                <facts argument="EVALUATION">
                  <value xsi:type="EM:StringValue" value="greater than"/>
                </facts>
                <facts argument="AGE1">
                  <value xsi:type="EM:IntegerValue" intValue="1"/>
                </facts>
                <facts argument="UNIT1">
                  <value xsi:type="EM:StringValue" value="years"/>
                </facts>
                <facts argument="OPT_AGE2"/>
                <facts argument="OPT_UNIT2">
                  <value xsi:type="EM:StringValue"/>
                </facts>
                <facts argument="OPT_LINK">
                  <value xsi:type="EM:AntecedentReference"/>
                </facts>
              </expression>
            </leftExpression>
            <rightExpression xsi:type="EM:Antecedent" id="L3" template="EKS_LOGIC_TRUE">
              <facts argument="LOGIC">
                <value xsi:type="EM:StringValue" value="@quot;LOGIC@quot;"/>
              </facts>
            </rightExpression>
          </expression>
        </rightExpression>
      </logicExpression>
    </logicSection>
    <actionSection>
      <action xsi:type="EM:ActionGroups">
        <actionGroups>
          <logicExpression xsi:type="EM:LogicReference" antecedent="//@ruleDefinition/@logicSection/@logicExpression/@rightExpression/@expression/@leftExpression/@expression"/>
          <actionExpression>
            <logicExpression xsi:type="EM:Antecedent" id="A1" template="EKS_ALERT_FLEX_A">
              <facts argument="OPT_TITLE">
                <value xsi:type="EM:StringValue" value="SUPPRESS"/>
              </facts>
              <facts argument="TXT">
                <value xsi:type="EM:StringValue" value="SUPPRESSED"/>
              </facts>
              <facts argument="OPT_CANCEL_LABEL">
                <value xsi:type="EM:StringValue"/>
              </facts>
              <facts argument="OPT_IGNORE_LABEL">
                <value xsi:type="EM:StringValue"/>
              </facts>
              <facts argument="OPT_OVERRIDE_REASON">
                <value xsi:type="EM:StringValue"/>
              </facts>
              <facts argument="OPT_MODIFY_LABEL">
                <value xsi:type="EM:StringValue"/>
              </facts>
              <facts argument="OPT_DEFAULT_LABEL">
                <value xsi:type="EM:StringValue" value="NONE"/>
              </facts>
              <facts argument="OPT_ORDERS">
                <value xsi:type="EM:StringValue"/>
              </facts>
              <facts argument="OPT_DEFAULT_FIRST_ORDER">
                <value xsi:type="EM:StringValue" value="DISABLED"/>
              </facts>
              <facts argument="OPT_URL_BUTTON_NAME">
                <value xsi:type="EM:StringValue"/>
              </facts>
              <facts argument="OPT_URL_ADDRESS">
                <value xsi:type="EM:StringValue"/>
              </facts>
              <facts argument="OK_BUTTON_NAME">
                <value xsi:type="EM:StringValue" value="OK"/>
              </facts>
              <facts argument="OPT_FORM">
                <value xsi:type="EM:StringValue"/>
              </facts>
              <facts argument="OPT_FORM_BUTTON_NAME">
                <value xsi:type="EM:StringValue"/>
              </facts>
              <facts argument="OPT_FORCE_FORM">
                <value xsi:type="EM:StringValue" value="ENABLED"/>
              </facts>
              <facts argument="OPT_LINK">
                <value xsi:type="EM:AntecedentReference"/>
              </facts>
            </logicExpression>
          </actionExpression>
        </actionGroups>
      </action>
    </actionSection>
  </ruleDefinition>
