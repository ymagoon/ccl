    </logicSection>
    <actionSection>
      <action xsi:type="EM:ActionExpression">
        <logicExpression xsi:type="EM:InfixExpression">
          <leftExpression xsi:type="EM:Antecedent" id="A1" template="EKS_SETUP_ADD_ORDERS_A">
            <facts argument="GROUP_NAME">
              <value xsi:type="EM:StringValue" value="Incomplete ADB"/>
            </facts>
            <facts argument="ORDERS">
              <value xsi:type="EM:ListValue">
                <values xsi:type="EM:ListValue">
                  <values xsi:type="EM:CustomValue" hideDisplay="true">
                    <customValue xsi:type="EM:StringValue" value="393574870.0|393574860.0|305346227.0|Incomplete Admission Database|2515.0"/>
                  </values>
                  <values xsi:type="EM:StringValue" value="Incomplete Admission Database"/>
                  <values xsi:type="EM:ListValue">
                    <values xsi:type="EM:CustomValue" hideDisplay="true">
                      <customValue xsi:type="EM:StringValue" value="2515.0"/>
                    </values>
                    <values xsi:type="EM:CustomValue" hideDisplay="true">
                      <customValue xsi:type="EM:StringValue" value="I|2515.0"/>
                    </values>
                  </values>
                </values>
              </value>
            </facts>
            <facts argument="OPT_COMMENT_TYPE">
