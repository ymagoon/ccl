      </evokes>
    </evokeSection>
    <logicSection>
      <logicExpression xsi:type="EM:InfixExpression">
        <leftExpression xsi:type="EM:Antecedent" id="L1" template="EKS_COLL_STATUS">
          <facts argument="PROCEDURE">
            <value xsi:type="EM:StringValue" value="UA Reflex MICRO and CULT"/>
          </facts>
          <facts argument="STATUS">
            <value xsi:type="EM:StringValue" value="In-Lab"/>
          </facts>
        </leftExpression>
        <rightExpression xsi:type="EM:InfixExpression">
          <leftExpression xsi:type="EM:Antecedent" id="L2" template="EKS_AGE_EVAL">
            <facts argument="EVALUATION">
              <value xsi:type="EM:StringValue" value="less than"/>
            </facts>
            <facts argument="AGE">
              <value xsi:type="EM:IntegerValue" intValue="1"/>
            </facts>
            <facts argument="QUALIFIER">
              <value xsi:type="EM:StringValue" value="years"/>
            </facts>
          </leftExpression>
          <rightExpression xsi:type="EM:InfixExpression">
            <leftExpression xsi:type="EM:Antecedent" id="L3" template="EKS_PT_LOCATION">
              <facts argument="LTYPE">
                <value xsi:type="EM:StringValue" value="FACILITY"/>
              </facts>
              <facts argument="LOCATION">
                <value xsi:type="EM:StringValue" value="SFB"/>
              </facts>
            </leftExpression>
            <rightExpression xsi:type="EM:PrefixExpression">
              <expression xsi:type="EM:Antecedent" id="L4" template="EKS_ORDERS_FIND_L">
                <facts argument="ORD_METHOD">
                  <value xsi:type="EM:StringValue" value="whose primary mnemonic is"/>
                </facts>
                <facts argument="OPT_ORDERS">
                  <value xsi:type="EM:ListValue">
                    <values xsi:type="EM:ListValue">
                      <values xsi:type="EM:CustomValue" hideDisplay="true">
