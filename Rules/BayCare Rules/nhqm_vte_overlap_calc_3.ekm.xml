        </arguments>
        <arguments xsi:type="EM:FixedListArgument" name="EVALUATION" dataType="CHARACTER" validationType="READ_ONLY" required="true" reconcileScript="N/A">
          <stringTable>
            <column value="EQUAL TO"/>
