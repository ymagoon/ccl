      <templateDefinitions xsi:type="EM:ExpertTemplate" name="EKS_RESULT_STATUS" definition="a  RESULT_STATUS result exists for PROCEDURE" description="Incoming discrete task assay, status" eventDependency="//@expertTemplates/@events/@event.5 //@expertTemplates/@events/@event.12 //@expertTemplates/@events/@event.38 //@expertTemplates/@events/@event.56">
        <keywords>PATHNET</keywords>
