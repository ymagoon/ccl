      <templateDefinitions xsi:type="EM:ExpertTemplate" name="EKS_CE_RESULT_INCOMING_REQ_L" definition="the triggering request contains a STATUS result for EVENT_SET_NAME that is EVALUATION VALUE1 and OPT_VALUE2" description="Incoming numeric result value, status" eventDependency="//@expertTemplates/@events/@event.8" optimizable="true" reconcilable="true">
