								<concat destfile="${project.build.directory}/compressed-resources/js/locale/${i18n.fr.filename}" fixlastline="true" eol="cr" append="yes" encoding="UTF-8">
									<fileset dir="${basedir}/src/main/resources/driver/js/i18n" includes="*.js" />
									<fileset dir="${project.build.directory}/external-resources/i18n" includes="*.js" />
									<fileset dir="${project.build.directory}/external-resources/i18n/fr" includes="*.js" />
									<fileset dir="${project.build.directory}/external-resources/i18n" includes="rcm*.js" />
									<fileset dir="${project.build.directory}/external-resources/i18n" includes="formatter_locale.js" />
								</concat>
								
								<!-- copy user customization over to build -->
								<copy todir="${project.build.directory}/compressed-resources/user-customization">
									<fileset dir="${project.build.directory}/external-resources/user-customization" />
								</copy>

								<!-- copy discrete graphing over to build -->
								<copy todir="${project.build.directory}/compressed-resources/discrete-graphing">
									<fileset dir="${project.build.directory}/external-resources/discrete-graphing" />
								</copy>

								<!-- copy images to root folder -->
								<copy todir="${project.build.directory}/compressed-resources/images">
									<fileset dir="${project.build.directory}/external-resources/images" />
								</copy>
							</target>
						</configuration>
						<goals>
							<goal>run</goal>
						</goals>
					</execution>
				</executions>
				<dependencies>
					<dependency>
						<groupId>ant-contrib</groupId>
						<artifactId>ant-contrib</artifactId>
						<version>1.0b3</version>
						<exclusions>
							<exclusion>
								<groupId>ant</groupId>
								<artifactId>ant</artifactId>
							</exclusion>
						</exclusions>
					</dependency>
				</dependencies>
			</plugin>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-enforcer-plugin</artifactId>
				<version>1.0-beta-1</version>
				<executions>
					<execution>
						<id>enforce-execution-environment</id>
						<goals>
							<goal>enforce</goal>
						</goals>
						<configuration>
							<rules>
								<requireMavenVersion>
									<version>[3.0.3,)</version>
								</requireMavenVersion>
								<requireJavaVersion>
									<version>[1.6,)</version>
								</requireJavaVersion>
							</rules>
						</configuration>
					</execution>
				</executions>
			</plugin>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-compiler-plugin</artifactId>
				<version>2.0.2</version>
				<configuration>
					<source>1.5</source>
					<target>1.5</target>
					<encoding>UTF-8</encoding>
					<showDeprecation>true</showDeprecation>
				</configuration>
			</plugin>
		</plugins>
	</build>
