/*
 * NAME:  bbt_get_pathnet_seq_sub.inc
 * 
 * PURPOSE:  retrieve next "pathnet_seq" value
 *
 * USE:
 *	1.  In calling program (only once, probably at top or bottom):
 *		%i cclsource:get_pathnet_seq_sub.inc
 *	2.  To get next "pathnet_seq" value:
 *		<double variable> = NEXT_PATHNET_SEQ(0)
 *		if (curqual = 0)
 *		    <error processing>
 *		else
 *		    <continue processing...<double variable> = new pathnet_seq value>
 *
 * jow - 03/19/1997
 */

DECLARE NEXT_PATHNET_SEQ(pathnet_seq_dummy) = f8
DECLARE new_pathnet_seq = f8 with protect, noconstant(0.0)

SUBROUTINE NEXT_PATHNET_SEQ(pathnet_seq_dummy)
    SET new_pathnet_seq = 0.0

    SELECT INTO "nl:"
        seqn        = seq(pathnet_seq,nextval)
    FROM
        dual
    DETAIL
         new_pathnet_seq        = seqn
    WITH
        format, nocounter

    RETURN(new_pathnet_seq)
END		; end subroutine NEXT_PATHNET_SEQ

